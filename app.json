{
  "expo": {
    "name": "WORDLLE",
    "slug": "wordlle",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "wordlle",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mckenytech.wordlle"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "versionCode": 1,
      "package": "com.mckenytech.wordlle"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "web": {
        "bundler": "metro"
      }
    },
    "extra": {
      "eas": {
        "projectId": "5e1fa144-5732-4bb2-9304-2d9be58e7d89"
      }
    },
    "plugins": [
      "expo-community-flipper"
    ]
  }
}