{
  "expo": {
    "name": "WORDLLE",
    "slug": "wordlle",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "wordlle",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#223EC1"
    },
    "updates": {
      "fallbackToCacheTimeout": 100000,
      "url": "https://u.expo.dev/5e1fa144-5732-4bb2-9304-2d9be58e7d89"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mckenytech.wordlle",
      "runtimeVersion": {
        "policy": "sdkVersion"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#223EC1"
      },
      "versionCode": 3,
      "package": "com.mckenytech.wordlle",
      "runtimeVersion": "1.0.2"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "web": {
        "bundler": "metro"
      }
    },
    "privacy": "public",
    "extra": {
      "eas": {
        "projectId": "5e1fa144-5732-4bb2-9304-2d9be58e7d89"
      }
    },
    "plugins": [
      "expo-community-flipper"
    ]
  }
}